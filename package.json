{
  "name": "anmol",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev --host ",
    "clean": "rm -rf dist/",
    "build": "astro build && cp ./robots.txt ./public/ ",
    "deploy": "wrangler pages deploy --project-name anmol-fzr ./dist",
    "open": "xdg-open 'https://withanmol.com'",
